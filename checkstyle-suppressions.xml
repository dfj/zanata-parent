<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
  "-//Puppy Crawl//DTD Suppressions 1.1//EN"
  "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
  <suppress checks=".*" files=".*(compressed.js|min.js|\.eot|\.gif|\.ico|\.patch|\.png|\.ttf|\.woff)" />
  <suppress checks="FileTabCharacter|RegexpSingleline" files=".*/LICENSE.txt" />
  <suppress checks="FileTabCharacter" files=".*/src/test/.*\.(xml|po|pot)" />
  <suppress checks="JavadocMethodCheck|JavadocTypeCheck" files=".*\.java" />
  <suppress checks="MagicNumberCheck" files=".*Test\.java" />
</suppressions>
